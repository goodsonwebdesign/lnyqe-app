    "docker:prod": "docker-compose up app-prod",
    "docker:prod:build": "docker-compose build app-prod",
    "ci:test": "ng test --watch=false --browsers=ChromeHeadless",
    "ci:lint": "ng lint",
    "ci:build": "ng build --configuration production",
    "format": "prettier --write \"src/**/*.{ts,js,json,scss,css,html,md}\"",
    "prepare": "husky",
    "lint": "ng lint"
  },
  "private": true,
